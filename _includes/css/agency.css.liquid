header {
    text-align: center;
    color: #{{ site.data.template.color.secondary-normal }};
    background-attachment: scroll;
    background-image: url('{{ site.baseurl }}/images/header-bg.jpg');
    background-position: center center;
    background-repeat: none;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    background-size: cover;
    -o-background-size: cover;
    position: relative;
}

header .intro-text {
    padding-top: 100px;
    padding-bottom: 50px;
}

header .intro-text .intro-lead-in {
    margin-bottom: 25px;
    font-family: {{ site.data.template.font.primary }};
    font-size: 22px;
    font-style: italic;
    line-height: 22px;
    display: none;
}

header .intro-text .intro-heading {
    margin-bottom: 25px;
    text-transform: uppercase;
    font-family: {{ site.data.template.font.primary }};
    font-size: 50px;
    font-weight: 700;
    line-height: 50px;
    display: none;
}

@media(min-width:768px) {
    header .intro-text {
        padding-top: 300px;
        padding-bottom: 200px;
    }

    header .intro-text .intro-lead-in {
        margin-bottom: 25px;
        font-family: {{ site.data.template.font.primary }};
        font-size: 40px;
        font-style: italic;
        line-height: 40px;
    }

    header .intro-text .intro-heading {
        margin-bottom: 50px;
        text-transform: uppercase;
        font-family: {{ site.data.template.font.primary }};
        font-size: 75px;
        font-weight: 700;
        line-height: 75px;
    }
}

/* Position the intro call-to-action at the bottom center of the banner
   with the same spacing as the navbar padding for visual symmetry */
header .intro-cta {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 45px; /* matches .navbar-default padding */
}

/* When navbar shrinks, reduce the bottom spacing to match the shrunk navbar padding */
.navbar-default.navbar-shrink + header .intro-cta {
    bottom: 45px;
}

/* Mobile: place the CTA after the intro text with top spacing so it doesn't overlap */
@media (max-width:767px) {
    /* Ensure peader content clears the fixed navbar on small screens */
    header {
        padding-top: 70px;
    }

}

